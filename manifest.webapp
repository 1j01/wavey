{
	"name": "Wavey",
	"version": "0.0.0",
	"description": "Edit audio simply",
	"icons": {
		"16": "images/icon-16.png",
		"48": "images/icon-48.png",
		"128": "images/icon-128.png"
	},
	"activities": {
		"edit": {
			"filters": {
				"type": [ "audio/*" ]
			},
			"href": "/",
			"disposition": "window",
			"returnValue": true
		}
	},
	"permissions": {},
	"developer": {
		"name": "Isaiah Odhner",
		"url": "https://isaiahodhner.io/"
	},
	"launch_path": "/",
	"default_locale": "en"
}
